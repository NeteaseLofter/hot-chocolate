{
  "name": "@hot-chocolate/examples",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "keywords": [
    "sandbox",
    "micro frontend",
    "micro-frontend",
    "microfrontend"
  ],
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "install-main": "cd mainApplication && npm install",
    "install-sub-vue": "cd vue2 && npm install",
    "install-sub-react15": "cd react15 && npm install",
    "install-sub-react16": "cd react16+ && npm install",
    "load-main": "cd mainApplication && npm start",
    "load-sub-vue": "cd vue2 && npm start",
    "load-sub-react15": "cd react15 && npm start",
    "load-sub-react16": "cd react16+ && npm start",
    "install": "concurrently \"npm run install-main\" \"npm run install-sub-vue\" \"npm run install-sub-react15\"\"npm run install-sub-react16\" ",
    "start": "concurrently \"npm run load-main\" \"npm run load-sub-vue\" \"npm run load-sub-react15\""
  },
  "license": "MIT",
