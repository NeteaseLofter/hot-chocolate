    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "axios": "^0.21.1",
    "hot-chocolate": "^0.1.8-alpha.0",
